-----codigo------

let parImpar = prompt("Digite o numero para ver se é par ou impar:")
converterParParaImpar(parImpar)


function converterParParaImpar (numero) {
    let resultado
    if (numero % 2 == 0) {
        resultado = "par"
    }else {
        resultado = "impar"
    }
    console.log(`O numero ${numero} é ${resultado}`)
}

teste 1: digitei 6 e resultou em par
teste 2: digitei 7 e resultou em impar
teste 3: digitei 4 e resultou em par
teste 4: digitei 465 e resultou em impar
teste 5: digitei 4516 e resultou em par


